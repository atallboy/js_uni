<view><view class="tab-container" style="position:fixed;left:0;right:0;top:0;z-index:999;background-color:#fff;"><scroll-view scroll-x="true" scroll-left="{{scrollLeft}}" bindscrolltoupper="upper" bindscrolltolower="lower" bindscroll="scroll" show-scrollbar="false"><view class="tab-wrap"><block wx:for="{{cate}}" wx:for-item="v" wx:for-index="k"><block><view data-event-opts="{{[['tap',[['changeCate',['$0',k],[[['cate','',k]]]]]]]}}" class="{{['tab-item '+v.active]}}" bindtap="__e"><view class="tab-txt">{{v.name}}</view><view class="tab-bar"></view></view></block></block></view></scroll-view></view><view style="height:45px;"></view><block wx:if="{{!list[0]}}"><view style="text-align:center;padding-top:100px;"><image style="width:200px;height:200px;" src="../../static/images/null.png"></image></view></block><view class="order-container"><view class="order-wrap"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="k"><view class="order-item"><view class="order-item-header"><view class="order-item-status">{{item.status_msg}}</view><view class="order-item-master"></view></view><block wx:for="{{item.order_product}}" wx:for-item="v" wx:for-index="__i0__"><view class="order-item-name">{{v.name}}</view></block><view class="order-item-info"><view class="order-item-time">{{"服务时间："+item.service_time}}</view><view class="order-item-price">{{item.total_fee}}</view></view><view class="order-item-act mix-top"><view data-event-opts="{{[['tap',[['detail',['$0'],[[['list','',k,'id']]]]]]]}}" class="order-item-btn" bindtap="__e">查看详情</view><block wx:if="{{item.status==0}}"><view data-event-opts="{{[['tap',[['pay',['$0'],[[['list','',k,'id']]]]]]]}}" class="order-item-btn" bindtap="__e">立即支付</view></block></view></view></block></view></view></view>